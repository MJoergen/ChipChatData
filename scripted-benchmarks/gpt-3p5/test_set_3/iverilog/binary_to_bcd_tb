#! /usr/bin/vvp
:ivl_version "12.0 (stable)" "(v12_0-dirty)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/ivl/system.vpi";
:vpi_module "/usr/lib/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/ivl/va_math.vpi";
S_0x55d2efc68d70 .scope module, "BinaryToBCD_tb" "BinaryToBCD_tb" 2 1;
 .timescale 0 0;
v0x55d2efc7e160_0 .net "bcd_output", 7 0, v0x55d2efc58150_0;  1 drivers
v0x55d2efc7e220_0 .var "binary_input", 4 0;
v0x55d2efc7e2c0_0 .var "expected_output", 7 0;
v0x55d2efc7e390_0 .var/i "num_tests", 31 0;
S_0x55d2efc68f00 .scope task, "check_output" "check_output" 2 38, 2 38 0, S_0x55d2efc68d70;
 .timescale 0 0;
TD_BinaryToBCD_tb.check_output ;
    %load/vec4 v0x55d2efc7e160_0;
    %load/vec4 v0x55d2efc7e2c0_0;
    %cmp/ne;
    %jmp/0xz  T_0.0, 6;
    %vpi_call 2 41 "$display", "Test case %d failed!", v0x55d2efc7e390_0 {0 0 0};
    %vpi_call 2 42 "$display", "Input: %b, Expected BCD Output: %b, Actual BCD Output: %b", v0x55d2efc7e220_0, v0x55d2efc7e2c0_0, v0x55d2efc7e160_0 {0 0 0};
T_0.0 ;
    %end;
S_0x55d2efc57f50 .scope module, "dut" "BinaryToBCD" 2 6, 3 1 0, S_0x55d2efc68d70;
 .timescale 0 0;
    .port_info 0 /INPUT 5 "binary_input";
    .port_info 1 /OUTPUT 8 "bcd_output";
v0x55d2efc58150_0 .var "bcd_output", 7 0;
v0x55d2efc7e020_0 .net "binary_input", 4 0, v0x55d2efc7e220_0;  1 drivers
E_0x55d2efc66bc0 .event anyedge, v0x55d2efc7e020_0;
    .scope S_0x55d2efc57f50;
T_1 ;
    %wait E_0x55d2efc66bc0;
    %load/vec4 v0x55d2efc7e020_0;
    %dup/vec4;
    %pushi/vec4 0, 0, 5;
    %cmp/u;
    %jmp/1 T_1.0, 6;
    %dup/vec4;
    %pushi/vec4 1, 0, 5;
    %cmp/u;
    %jmp/1 T_1.1, 6;
    %dup/vec4;
    %pushi/vec4 2, 0, 5;
    %cmp/u;
    %jmp/1 T_1.2, 6;
    %dup/vec4;
    %pushi/vec4 3, 0, 5;
    %cmp/u;
    %jmp/1 T_1.3, 6;
    %dup/vec4;
    %pushi/vec4 4, 0, 5;
    %cmp/u;
    %jmp/1 T_1.4, 6;
    %dup/vec4;
    %pushi/vec4 5, 0, 5;
    %cmp/u;
    %jmp/1 T_1.5, 6;
    %dup/vec4;
    %pushi/vec4 6, 0, 5;
    %cmp/u;
    %jmp/1 T_1.6, 6;
    %dup/vec4;
    %pushi/vec4 7, 0, 5;
    %cmp/u;
    %jmp/1 T_1.7, 6;
    %dup/vec4;
    %pushi/vec4 8, 0, 5;
    %cmp/u;
    %jmp/1 T_1.8, 6;
    %dup/vec4;
    %pushi/vec4 9, 0, 5;
    %cmp/u;
    %jmp/1 T_1.9, 6;
    %pushi/vec4 255, 255, 8;
    %store/vec4 v0x55d2efc58150_0, 0, 8;
    %jmp T_1.11;
T_1.0 ;
    %pushi/vec4 0, 0, 8;
    %store/vec4 v0x55d2efc58150_0, 0, 8;
    %jmp T_1.11;
T_1.1 ;
    %pushi/vec4 1, 0, 8;
    %store/vec4 v0x55d2efc58150_0, 0, 8;
    %jmp T_1.11;
T_1.2 ;
    %pushi/vec4 2, 0, 8;
    %store/vec4 v0x55d2efc58150_0, 0, 8;
    %jmp T_1.11;
T_1.3 ;
    %pushi/vec4 3, 0, 8;
    %store/vec4 v0x55d2efc58150_0, 0, 8;
    %jmp T_1.11;
T_1.4 ;
    %pushi/vec4 4, 0, 8;
    %store/vec4 v0x55d2efc58150_0, 0, 8;
    %jmp T_1.11;
T_1.5 ;
    %pushi/vec4 5, 0, 8;
    %store/vec4 v0x55d2efc58150_0, 0, 8;
    %jmp T_1.11;
T_1.6 ;
    %pushi/vec4 6, 0, 8;
    %store/vec4 v0x55d2efc58150_0, 0, 8;
    %jmp T_1.11;
T_1.7 ;
    %pushi/vec4 7, 0, 8;
    %store/vec4 v0x55d2efc58150_0, 0, 8;
    %jmp T_1.11;
T_1.8 ;
    %pushi/vec4 8, 0, 8;
    %store/vec4 v0x55d2efc58150_0, 0, 8;
    %jmp T_1.11;
T_1.9 ;
    %pushi/vec4 9, 0, 8;
    %store/vec4 v0x55d2efc58150_0, 0, 8;
    %jmp T_1.11;
T_1.11 ;
    %pop/vec4 1;
    %jmp T_1;
    .thread T_1, $push;
    .scope S_0x55d2efc68d70;
T_2 ;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x55d2efc7e390_0, 0, 32;
    %pushi/vec4 0, 0, 5;
    %store/vec4 v0x55d2efc7e220_0, 0, 5;
    %pushi/vec4 0, 0, 8;
    %store/vec4 v0x55d2efc7e2c0_0, 0, 8;
    %delay 10, 0;
    %load/vec4 v0x55d2efc7e390_0;
    %addi 1, 0, 32;
    %store/vec4 v0x55d2efc7e390_0, 0, 32;
    %fork TD_BinaryToBCD_tb.check_output, S_0x55d2efc68f00;
    %join;
    %pushi/vec4 3, 0, 5;
    %store/vec4 v0x55d2efc7e220_0, 0, 5;
    %pushi/vec4 3, 0, 8;
    %store/vec4 v0x55d2efc7e2c0_0, 0, 8;
    %delay 10, 0;
    %load/vec4 v0x55d2efc7e390_0;
    %addi 1, 0, 32;
    %store/vec4 v0x55d2efc7e390_0, 0, 32;
    %fork TD_BinaryToBCD_tb.check_output, S_0x55d2efc68f00;
    %join;
    %vpi_call 2 33 "$display", "All test cases passed! Total tests: %d", v0x55d2efc7e390_0 {0 0 0};
    %vpi_call 2 34 "$finish" {0 0 0};
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
