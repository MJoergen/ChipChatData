v0x560120d09d70_0 .var "feedback", 0 0;
v0x560120d09e10_0 .net "reset", 0 0, L_0x560120d0a2b0;  1 drivers
v0x560120d09ed0_0 .var "state", 7 0;
E_0x560120cf0bd0/0 .event negedge, v0x560120d09e10_0;
E_0x560120cf0bd0/1 .event posedge, v0x560120d09bb0_0;
E_0x560120cf0bd0 .event/or E_0x560120cf0bd0/0, E_0x560120cf0bd0/1;
    .scope S_0x560120d09920;
T_0 ;
    %wait E_0x560120cf0bd0;
    %load/vec4 v0x560120d09e10_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 138, 0, 8;
    %assign/vec4 v0x560120d09ed0_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x560120d09ed0_0;
    %parti/s 1, 1, 2;
    %load/vec4 v0x560120d09ed0_0;
    %parti/s 1, 4, 4;
    %xor;
    %load/vec4 v0x560120d09ed0_0;
    %parti/s 1, 6, 4;
    %xor;
    %load/vec4 v0x560120d09ed0_0;
    %parti/s 1, 7, 4;
    %xor;
    %assign/vec4 v0x560120d09d70_0, 0;
    %load/vec4 v0x560120d09ed0_0;
    %parti/s 7, 0, 2;
    %load/vec4 v0x560120d09d70_0;
    %concat/vec4; draw_concat_vec4
    %assign/vec4 v0x560120d09ed0_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x560120ce2980;
T_1 ;
    %delay 5, 0;
    %load/vec4 v0x560120d0a080_0;
    %inv;
    %store/vec4 v0x560120d0a080_0, 0, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x560120ce2980;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x560120d0a080_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x560120d0a1c0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x560120d0a1c0_0, 0, 1;
    %load/vec4 v0x560120d0a120_0;
    %cmpi/e 138, 0, 8;
    %jmp/0xz  T_2.0, 6;
    %vpi_call 2 28 "$display", "Test case 1 passed! Initial state match." {0 0 0};
    %jmp T_2.1;
T_2.0 ;
    %vpi_call 2 30 "$display", "ERROR: Test case 1 failed! Initial state mismatch." {0 0 0};
    %vpi_call 2 31 "$finish" {0 0 0};
T_2.1 ;
    %fork t_1, S_0x560120ce2b10;
    %jmp t_0;
    .scope S_0x560120ce2b10;
t_1 ;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x560120ce2cf0_0, 0, 32;
T_2.2 ;
    %load/vec4 v0x560120ce2cf0_0;
    %cmpi/s 8, 0, 32;
    %jmp/0xz T_2.3, 5;
    %delay 10, 0;
    %load/vec4 v0x560120ce2cf0_0;
    %cmpi/e 7, 0, 32;
    %flag_get/vec4 6;
    %jmp/0 T_2.6, 6;
    %load/vec4 v0x560120d0a120_0;
    %pushi/vec4 197, 0, 8;
    %cmp/ne;
    %flag_get/vec4 6;
    %and;
T_2.6;
    %flag_set/vec4 8;
    %jmp/0xz  T_2.4, 8;
    %vpi_call 2 38 "$display", "ERROR: Test case 2 failed! Unexpected output at cycle %d.", v0x560120ce2cf0_0 {0 0 0};
    %vpi_call 2 39 "$finish" {0 0 0};
T_2.4 ;
    %load/vec4 v0x560120ce2cf0_0;
    %addi 1, 0, 32;
    %store/vec4 v0x560120ce2cf0_0, 0, 32;
    %jmp T_2.2;
T_2.3 ;
    %end;
    .scope S_0x560120ce2980;
t_0 %join;
    %vpi_call 2 43 "$display", "All test cases passed successfully." {0 0 0};
    %vpi_call 2 44 "$finish" {0 0 0};
    %end;
    .thread T_2;
