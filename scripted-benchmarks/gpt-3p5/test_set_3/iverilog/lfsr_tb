 .timescale 0 0;
v0x560120ce2cf0_0 .var/i "i", 31 0;
S_0x560120d09920 .scope module, "dut" "lfsr" 2 8, 3 1 0, S_0x560120ce2980;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "reset";
    .port_info 2 /OUTPUT 8 "data";
L_0x560120ce0550 .functor BUFZ 8, v0x560120d09ed0_0, C4<00000000>, C4<00000000>, C4<00000000>;
v0x560120d09bb0_0 .net "clk", 0 0, v0x560120d0a080_0;  1 drivers
