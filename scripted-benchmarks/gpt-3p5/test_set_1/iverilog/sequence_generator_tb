S_0x5604e28822f0 .scope task, "test_enable_high" "test_enable_high" 2 31, 2 31 0, S_0x5604e28646f0;
 .timescale 0 0;
v0x5604e28824f0_0 .var/i "i", 31 0;
TD_sequence_generator_tb.test_enable_high ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5604e2882950_0, 0, 1;
    %pushi/vec4 2, 0, 32;
T_0.0 %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/s;
    %jmp/1xz T_0.1, 5;
    %jmp/1 T_0.1, 4;
    %pushi/vec4 1, 0, 32;
    %sub;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x5604e28824f0_0, 0, 32;
T_0.2 ;
    %load/vec4 v0x5604e28824f0_0;
    %cmpi/s 8, 0, 32;
    %jmp/0xz T_0.3, 5;
    %delay 10, 0;
    %load/vec4 v0x5604e2882880_0;
    %ix/getv/s 4, v0x5604e28824f0_0;
    %load/vec4a v0x5604e2851e30, 4;
    %cmp/ne;
    %jmp/0xz  T_0.4, 6;
    %vpi_call 2 41 "$display", "Error: unexpected sequence value %h at index %d", v0x5604e2882880_0, v0x5604e28824f0_0 {0 0 0};
    %vpi_call 2 42 "$finish" {0 0 0};
T_0.4 ;
    %load/vec4 v0x5604e28824f0_0;
    %addi 1, 0, 32;
    %store/vec4 v0x5604e28824f0_0, 0, 32;
    %jmp T_0.2;
T_0.3 ;
    %jmp T_0.0;
T_0.1 ;
    %pop/vec4 1;
    %vpi_call 2 47 "$display", "Test Passed: Sequence generator output matches expected sequence with enable signal high" {0 0 0};
    %vpi_call 2 48 "$finish" {0 0 0};
    %end;
S_0x5604e28825d0 .scope task, "test_enable_low" "test_enable_low" 2 53, 2 53 0, S_0x5604e28646f0;
 .timescale 0 0;
TD_sequence_generator_tb.test_enable_low ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5604e2882950_0, 0, 1;
    %pushi/vec4 2, 0, 32;
T_1.6 %dup/vec4;
    %pushi/vec4 0, 0, 32;
    %cmp/s;
