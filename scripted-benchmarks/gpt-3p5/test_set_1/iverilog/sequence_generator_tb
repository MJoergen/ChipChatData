    %pad/u 32;
    %cmpi/ne 0, 0, 32;
    %jmp/0xz  T_1.8, 6;
    %vpi_call 2 62 "$display", "Error: non-zero data output when enable signal is low" {0 0 0};
    %vpi_call 2 63 "$finish" {0 0 0};
T_1.8 ;
