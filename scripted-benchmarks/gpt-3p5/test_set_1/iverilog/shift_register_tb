    .scope S_0x5568253d6360;
T_0 ;
    %wait E_0x5568253d43e0;
    %load/vec4 v0x5568253f0a30_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 8;
    %assign/vec4 v0x5568253f0970_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x5568253f0af0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.2, 8;
    %load/vec4 v0x5568253f0970_0;
    %parti/s 7, 0, 2;
    %load/vec4 v0x5568253f08b0_0;
    %concat/vec4; draw_concat_vec4
    %assign/vec4 v0x5568253f0970_0, 0;
T_0.2 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x5568253d61d0;
T_1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5568253f0ca0_0, 0, 1;
T_1.0 ;
    %delay 5000, 0;
    %load/vec4 v0x5568253f0ca0_0;
    %inv;
    %store/vec4 v0x5568253f0ca0_0, 0, 1;
    %jmp T_1.0;
    %end;
    .thread T_1;
    .scope S_0x5568253d61d0;
