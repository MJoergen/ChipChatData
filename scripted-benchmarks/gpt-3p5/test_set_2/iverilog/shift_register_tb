:vpi_module "/usr/lib/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/ivl/va_math.vpi";
S_0x55e640d0bd60 .scope module, "ShiftRegister_TB" "ShiftRegister_TB" 2 3;
 .timescale -9 -12;
v0x55e640d366d0_0 .var "clk", 0 0;
v0x55e640d36790_0 .var "data", 0 0;
v0x55e640d36860_0 .net "output_data", 7 0, v0x55e640d36140_0;  1 drivers
v0x55e640d36960_0 .var "reset", 0 0;
v0x55e640d36a30_0 .var "shift_enable", 0 0;
S_0x55e640d1d790 .scope module, "dut" "ShiftRegister" 2 12, 3 1 0, S_0x55e640d0bd60;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "reset";
    .port_info 2 /INPUT 1 "data";
    .port_info 3 /INPUT 1 "shift_enable";
    .port_info 4 /OUTPUT 8 "output_data";
v0x55e640d1d9d0_0 .net "clk", 0 0, v0x55e640d366d0_0;  1 drivers
v0x55e640d36080_0 .net "data", 0 0, v0x55e640d36790_0;  1 drivers
v0x55e640d36140_0 .var "output_data", 7 0;
v0x55e640d36200_0 .net "reset", 0 0, v0x55e640d36960_0;  1 drivers
v0x55e640d362c0_0 .var "reset_sync", 0 0;
v0x55e640d363d0_0 .net "shift_enable", 0 0, v0x55e640d36a30_0;  1 drivers
v0x55e640d36490_0 .var "synchronized_data", 7 0;
v0x55e640d36570_0 .var "synchronized_shift_enable", 0 0;
E_0x55e640d19c90/0 .event negedge, v0x55e640d36200_0;
E_0x55e640d19c90/1 .event posedge, v0x55e640d1d9d0_0;
E_0x55e640d19c90 .event/or E_0x55e640d19c90/0, E_0x55e640d19c90/1;
E_0x55e640d1a4c0 .event posedge, v0x55e640d1d9d0_0;
E_0x55e640d1a710/0 .event negedge, v0x55e640d362c0_0;
E_0x55e640d1a710/1 .event posedge, v0x55e640d1d9d0_0;
E_0x55e640d1a710 .event/or E_0x55e640d1a710/0, E_0x55e640d1a710/1;
    .scope S_0x55e640d1d790;
T_0 ;
    %wait E_0x55e640d1a710;
    %load/vec4 v0x55e640d362c0_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 8;
    %assign/vec4 v0x55e640d36490_0, 0;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x55e640d36570_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x55e640d36080_0;
    %pad/u 8;
    %assign/vec4 v0x55e640d36490_0, 0;
    %load/vec4 v0x55e640d363d0_0;
    %assign/vec4 v0x55e640d36570_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x55e640d1d790;
T_1 ;
    %wait E_0x55e640d1a4c0;
    %load/vec4 v0x55e640d362c0_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.0, 8;
    %pushi/vec4 0, 0, 8;
    %assign/vec4 v0x55e640d36140_0, 0;
    %jmp T_1.1;
T_1.0 ;
    %load/vec4 v0x55e640d36570_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.2, 8;
    %load/vec4 v0x55e640d36140_0;
    %parti/s 7, 0, 2;
    %load/vec4 v0x55e640d36490_0;
    %concat/vec4; draw_concat_vec4
    %pad/u 8;
    %assign/vec4 v0x55e640d36140_0, 0;
T_1.2 ;
T_1.1 ;
    %jmp T_1;
    .thread T_1;
    .scope S_0x55e640d1d790;
T_2 ;
    %wait E_0x55e640d19c90;
    %load/vec4 v0x55e640d36200_0;
    %inv;
    %assign/vec4 v0x55e640d362c0_0, 0;
    %jmp T_2;
    .thread T_2;
    .scope S_0x55e640d0bd60;
T_3 ;
    %delay 5000, 0;
    %load/vec4 v0x55e640d366d0_0;
    %inv;
    %store/vec4 v0x55e640d366d0_0, 0, 1;
    %jmp T_3;
    .thread T_3;
    .scope S_0x55e640d0bd60;
T_4 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55e640d366d0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55e640d36960_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55e640d36790_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55e640d36a30_0, 0, 1;
    %delay 10000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55e640d36960_0, 0, 1;
    %delay 20000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55e640d36960_0, 0, 1;
    %delay 10000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55e640d36790_0, 0, 1;
    %delay 5000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55e640d36a30_0, 0, 1;
    %delay 5000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55e640d36a30_0, 0, 1;
    %delay 25000, 0;
    %load/vec4 v0x55e640d36860_0;
    %cmpi/ne 1, 0, 8;
    %jmp/0xz  T_4.0, 6;
    %vpi_call 2 43 "$display", "Test case 1 failed: Expected 8'b00000001, Actual %b", v0x55e640d36860_0 {0 0 0};
T_4.0 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55e640d36790_0, 0, 1;
    %delay 5000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55e640d36a30_0, 0, 1;
