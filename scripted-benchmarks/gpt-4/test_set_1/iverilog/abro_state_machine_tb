S_0x5609649dac50 .scope module, "tb_abro_state_machine" "tb_abro_state_machine" 2 3;
 .timescale -9 -12;
P_0x5609649dade0 .param/l "CLK_PERIOD" 0 2 23, +C4<00000000000000000000000000001010>;
v0x5609649fa7f0_0 .var "A", 0 0;
v0x5609649fa8b0_0 .var "B", 0 0;
v0x5609649fa950_0 .net "O", 0 0, L_0x5609649faf20;  1 drivers
