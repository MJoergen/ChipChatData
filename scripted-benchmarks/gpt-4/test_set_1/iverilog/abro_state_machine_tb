v0x5609649facc0_0 .net "state", 3 0, v0x5609649fa650_0;  1 drivers
v0x5609649fad90_0 .var/i "test_case", 31 0;
v0x5609649fae30 .array "test_cases", 7 0, 7 0;
S_0x5609649dae80 .scope module, "uut" "abro_state_machine" 2 13, 3 1 0, S_0x5609649dac50;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "reset_n";
    .port_info 2 /INPUT 1 "A";
    .port_info 3 /INPUT 1 "B";
    .port_info 4 /OUTPUT 1 "O";
    .port_info 5 /OUTPUT 4 "state";
P_0x5609649c74b0 .param/l "IDLE" 1 3 11, C4<0001>;
P_0x5609649c74f0 .param/l "STATE_A" 1 3 12, C4<0010>;
P_0x5609649c7530 .param/l "STATE_B" 1 3 13, C4<0100>;
P_0x5609649c7570 .param/l "STATE_O" 1 3 14, C4<1000>;
v0x5609649cdeb0_0 .net "A", 0 0, v0x5609649fa7f0_0;  1 drivers
v0x5609649fa240_0 .net "B", 0 0, v0x5609649fa8b0_0;  1 drivers
v0x5609649fa300_0 .net "O", 0 0, L_0x5609649faf20;  alias, 1 drivers
L_0x7f334979c018 .functor BUFT 1, C4<1000>, C4<0>, C4<0>, C4<0>;
v0x5609649fa3a0_0 .net/2u *"_ivl_0", 3 0, L_0x7f334979c018;  1 drivers
v0x5609649fa480_0 .net "clk", 0 0, v0x5609649fa9f0_0;  1 drivers
v0x5609649fa590_0 .net "reset_n", 0 0, v0x5609649fac20_0;  1 drivers
v0x5609649fa650_0 .var "state", 3 0;
E_0x5609649d7310/0 .event negedge, v0x5609649fa590_0;
E_0x5609649d7310/1 .event posedge, v0x5609649fa480_0;
E_0x5609649d7310 .event/or E_0x5609649d7310/0, E_0x5609649d7310/1;
L_0x5609649faf20 .cmp/eq 4, v0x5609649fa650_0, L_0x7f334979c018;
    .scope S_0x5609649dae80;
T_0 ;
    %wait E_0x5609649d7310;
    %load/vec4 v0x5609649fa590_0;
    %inv;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 1, 0, 4;
    %assign/vec4 v0x5609649fa650_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x5609649fa650_0;
    %dup/vec4;
    %pushi/vec4 1, 0, 4;
    %cmp/u;
    %jmp/1 T_0.2, 6;
    %dup/vec4;
    %pushi/vec4 2, 0, 4;
    %cmp/u;
    %jmp/1 T_0.3, 6;
    %dup/vec4;
    %pushi/vec4 4, 0, 4;
    %cmp/u;
    %jmp/1 T_0.4, 6;
    %dup/vec4;
    %pushi/vec4 8, 0, 4;
    %cmp/u;
    %jmp/1 T_0.5, 6;
    %jmp T_0.6;
T_0.2 ;
    %load/vec4 v0x5609649cdeb0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.7, 8;
    %pushi/vec4 2, 0, 4;
    %assign/vec4 v0x5609649fa650_0, 0;
    %jmp T_0.8;
T_0.7 ;
    %load/vec4 v0x5609649fa240_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.9, 8;
    %pushi/vec4 4, 0, 4;
    %assign/vec4 v0x5609649fa650_0, 0;
T_0.9 ;
T_0.8 ;
    %jmp T_0.6;
T_0.3 ;
    %load/vec4 v0x5609649fa240_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.11, 8;
    %pushi/vec4 4, 0, 4;
    %assign/vec4 v0x5609649fa650_0, 0;
    %jmp T_0.12;
T_0.11 ;
    %pushi/vec4 1, 0, 4;
    %assign/vec4 v0x5609649fa650_0, 0;
T_0.12 ;
    %jmp T_0.6;
T_0.4 ;
    %load/vec4 v0x5609649cdeb0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.13, 8;
    %pushi/vec4 8, 0, 4;
    %assign/vec4 v0x5609649fa650_0, 0;
    %jmp T_0.14;
T_0.13 ;
    %pushi/vec4 1, 0, 4;
    %assign/vec4 v0x5609649fa650_0, 0;
T_0.14 ;
    %jmp T_0.6;
T_0.5 ;
    %pushi/vec4 1, 0, 4;
    %assign/vec4 v0x5609649fa650_0, 0;
    %jmp T_0.6;
T_0.6 ;
    %pop/vec4 1;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x5609649dac50;
T_1 ;
    %delay 5000, 0;
    %load/vec4 v0x5609649fa9f0_0;
    %inv;
    %store/vec4 v0x5609649fa9f0_0, 0, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x5609649dac50;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5609649fa9f0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5609649fac20_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5609649fa7f0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5609649fa8b0_0, 0, 1;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x5609649fad90_0, 0, 32;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x5609649faa90_0, 0, 32;
    %delay 10000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5609649fac20_0, 0, 1;
    %pushi/vec4 1, 0, 8;
    %ix/load 4, 0, 0;
    %flag_set/imm 4, 0;
    %store/vec4a v0x5609649fae30, 4, 0;
    %pushi/vec4 130, 0, 8;
    %ix/load 4, 1, 0;
    %flag_set/imm 4, 0;
    %store/vec4a v0x5609649fae30, 4, 0;
    %pushi/vec4 68, 0, 8;
    %ix/load 4, 2, 0;
    %flag_set/imm 4, 0;
    %store/vec4a v0x5609649fae30, 4, 0;
    %pushi/vec4 232, 0, 8;
    %ix/load 4, 3, 0;
    %flag_set/imm 4, 0;
    %store/vec4a v0x5609649fae30, 4, 0;
    %pushi/vec4 1, 0, 8;
    %ix/load 4, 4, 0;
    %flag_set/imm 4, 0;
    %store/vec4a v0x5609649fae30, 4, 0;
    %pushi/vec4 1, 0, 8;
    %ix/load 4, 5, 0;
    %flag_set/imm 4, 0;
    %store/vec4a v0x5609649fae30, 4, 0;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x5609649fab80_0, 0, 32;
T_2.0 ;
    %load/vec4 v0x5609649fab80_0;
    %cmpi/s 6, 0, 32;
    %jmp/0xz T_2.1, 5;
    %ix/getv/s 4, v0x5609649fab80_0;
    %load/vec4a v0x5609649fae30, 4;
    %parti/s 1, 7, 4;
    %store/vec4 v0x5609649fa7f0_0, 0, 1;
    %ix/getv/s 4, v0x5609649fab80_0;
    %load/vec4a v0x5609649fae30, 4;
    %parti/s 1, 6, 4;
    %store/vec4 v0x5609649fa8b0_0, 0, 1;
    %delay 10000, 0;
    %load/vec4 v0x5609649fa950_0;
    %ix/getv/s 4, v0x5609649fab80_0;
    %load/vec4a v0x5609649fae30, 4;
    %parti/s 1, 5, 4;
    %cmp/ne;
    %jmp/1 T_2.4, 6;
    %flag_mov 8, 6;
    %load/vec4 v0x5609649facc0_0;
    %ix/getv/s 4, v0x5609649fab80_0;
    %load/vec4a v0x5609649fae30, 4;
    %parti/s 4, 0, 2;
    %cmp/ne;
    %flag_or 6, 8;
T_2.4;
    %jmp/0xz  T_2.2, 6;
    %vpi_call 2 64 "$display", "Error: Test case %0d failed", v0x5609649fab80_0 {0 0 0};
    %vpi_call 2 65 "$display", "  Inputs: A = %0d, B = %0d", v0x5609649fa7f0_0, v0x5609649fa8b0_0 {0 0 0};
    %ix/getv/s 4, v0x5609649fab80_0;
    %load/vec4a v0x5609649fae30, 4;
    %parti/s 1, 5, 4;
    %ix/getv/s 4, v0x5609649fab80_0;
    %load/vec4a v0x5609649fae30, 4;
    %parti/s 4, 0, 2;
    %vpi_call 2 66 "$display", "  Expected: O = %0d, state = 4'b%b", S<1,vec4,u1>, S<0,vec4,u4> {2 0 0};
    %vpi_call 2 67 "$display", "  Actual: O = %0d, state = 4'b%b", v0x5609649fa950_0, v0x5609649facc0_0 {0 0 0};
    %load/vec4 v0x5609649faa90_0;
    %addi 1, 0, 32;
    %store/vec4 v0x5609649faa90_0, 0, 32;
    %jmp T_2.3;
T_2.2 ;
    %vpi_call 2 70 "$display", "Test case %0d passed", v0x5609649fab80_0 {0 0 0};
T_2.3 ;
    %load/vec4 v0x5609649fab80_0;
    %addi 1, 0, 32;
    %store/vec4 v0x5609649fab80_0, 0, 32;
    %jmp T_2.0;
T_2.1 ;
    %load/vec4 v0x5609649faa90_0;
    %cmpi/e 0, 0, 32;
    %jmp/0xz  T_2.5, 4;
    %vpi_call 2 76 "$display", "All test cases passed!" {0 0 0};
    %jmp T_2.6;
T_2.5 ;
    %vpi_call 2 78 "$display", "%0d test case(s) failed", v0x5609649faa90_0 {0 0 0};
T_2.6 ;
    %vpi_call 2 82 "$finish" {0 0 0};
    %end;
    .thread T_2;
