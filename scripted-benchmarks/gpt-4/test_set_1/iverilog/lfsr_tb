E_0x55f0e72e9210/1 .event posedge, v0x55f0e733c670_0;
E_0x55f0e72e9210 .event/or E_0x55f0e72e9210/0, E_0x55f0e72e9210/1;
L_0x55f0e733cd50 .part v0x55f0e733c730_0, 0, 1;
L_0x55f0e733ce40 .part v0x55f0e733c730_0, 3, 1;
L_0x55f0e733cfb0 .part v0x55f0e733c730_0, 5, 1;
L_0x55f0e733d130 .part v0x55f0e733c730_0, 6, 1;
    .scope S_0x55f0e7322870;
T_0 ;
    %wait E_0x55f0e72e9210;
    %load/vec4 v0x55f0e733c8d0_0;
    %inv;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 138, 0, 8;
    %assign/vec4 v0x55f0e733c730_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x55f0e733c730_0;
    %parti/s 7, 0, 2;
    %load/vec4 v0x55f0e733c810_0;
    %concat/vec4; draw_concat_vec4
    %assign/vec4 v0x55f0e733c730_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x55f0e7313c90;
T_1 ;
    %delay 5000, 0;
