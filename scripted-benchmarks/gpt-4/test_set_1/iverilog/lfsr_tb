    %inv;
    %store/vec4 v0x55f0e733ca10_0, 0, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x55f0e7313c90;
T_2 ;
    %pushi/vec4 138, 0, 8;
    %ix/load 4, 0, 0;
    %flag_set/imm 4, 0;
    %store/vec4a v0x55f0e733cb50, 4, 0;
    %pushi/vec4 1, 0, 32;
    %store/vec4 v0x55f0e733cbf0_0, 0, 32;
T_2.0 ;
    %load/vec4 v0x55f0e733cbf0_0;
    %cmpi/s 256, 0, 32;
    %jmp/0xz T_2.1, 5;
    %load/vec4 v0x55f0e733cbf0_0;
    %subi 1, 0, 32;
    %ix/vec4/s 4;
    %load/vec4a v0x55f0e733cb50, 4;
    %parti/s 7, 0, 2;
    %load/vec4 v0x55f0e733cbf0_0;
    %subi 1, 0, 32;
    %ix/vec4/s 4;
    %load/vec4a v0x55f0e733cb50, 4;
    %parti/s 1, 0, 2;
    %load/vec4 v0x55f0e733cbf0_0;
    %subi 1, 0, 32;
    %ix/vec4/s 4;
    %load/vec4a v0x55f0e733cb50, 4;
    %parti/s 1, 3, 3;
    %xor;
    %load/vec4 v0x55f0e733cbf0_0;
    %subi 1, 0, 32;
    %ix/vec4/s 4;
    %load/vec4a v0x55f0e733cb50, 4;
    %parti/s 1, 5, 4;
    %xor;
    %load/vec4 v0x55f0e733cbf0_0;
    %subi 1, 0, 32;
    %ix/vec4/s 4;
    %load/vec4a v0x55f0e733cb50, 4;
    %parti/s 1, 6, 4;
    %xor;
    %concat/vec4; draw_concat_vec4
    %ix/getv/s 4, v0x55f0e733cbf0_0;
    %store/vec4a v0x55f0e733cb50, 4, 0;
    %load/vec4 v0x55f0e733cbf0_0;
    %addi 1, 0, 32;
    %store/vec4 v0x55f0e733cbf0_0, 0, 32;
    %jmp T_2.0;
T_2.1 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55f0e733ca10_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55f0e733ccb0_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55f0e733ccb0_0, 0, 1;
    %delay 10000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55f0e733ccb0_0, 0, 1;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x55f0e733cbf0_0, 0, 32;
T_2.2 ;
    %load/vec4 v0x55f0e733cbf0_0;
    %cmpi/s 256, 0, 32;
    %jmp/0xz T_2.3, 5;
    %wait E_0x55f0e7320780;
    %load/vec4 v0x55f0e733cab0_0;
    %ix/getv/s 4, v0x55f0e733cbf0_0;
    %load/vec4a v0x55f0e733cb50, 4;
    %cmp/ne;
