#! /usr/bin/vvp
:ivl_version "12.0 (stable)" "(v12_0-dirty)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/usr/lib/ivl/system.vpi";
:vpi_module "/usr/lib/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/ivl/va_math.vpi";
S_0x564a74f6f780 .scope module, "tb_binary_to_bcd" "tb_binary_to_bcd" 2 3;
 .timescale -9 -12;
v0x564a74f87ce0_0 .var "actual_bcd", 7 0;
v0x564a74f87dc0_0 .net "bcd_tens", 3 0, v0x564a74f5efc0_0;  1 drivers
v0x564a74f87e80_0 .net "bcd_units", 3 0, v0x564a74f877d0_0;  1 drivers
v0x564a74f87f80_0 .var "binary_input", 4 0;
v0x564a74f88050_0 .var/i "error_count", 31 0;
v0x564a74f88140_0 .var "expected_bcd", 7 0;
v0x564a74f88220_0 .var/i "i", 31 0;
S_0x564a74f6f910 .scope module, "b2b" "binary_to_bcd" 2 8, 3 1 0, S_0x564a74f6f780;
 .timescale 0 0;
    .port_info 0 /INPUT 5 "binary_input";
    .port_info 1 /OUTPUT 4 "bcd_tens";
    .port_info 2 /OUTPUT 4 "bcd_units";
v0x564a74f5efc0_0 .var "bcd_tens", 3 0;
v0x564a74f877d0_0 .var "bcd_units", 3 0;
v0x564a74f878b0_0 .var/i "bcd_value", 31 0;
v0x564a74f87970_0 .net "binary_input", 4 0, v0x564a74f87f80_0;  1 drivers
v0x564a74f87a50_0 .var/i "binary_value", 31 0;
v0x564a74f87b80_0 .var/i "i", 31 0;
E_0x564a74f34250 .event anyedge, v0x564a74f87970_0, v0x564a74f878b0_0, v0x564a74f87a50_0;
    .scope S_0x564a74f6f910;
T_0 ;
    %wait E_0x564a74f34250;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x564a74f878b0_0, 0, 32;
    %load/vec4 v0x564a74f87970_0;
    %pad/u 32;
    %store/vec4 v0x564a74f87a50_0, 0, 32;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x564a74f87b80_0, 0, 32;
T_0.0 ;
    %load/vec4 v0x564a74f87b80_0;
    %cmpi/s 5, 0, 32;
    %jmp/0xz T_0.1, 5;
    %load/vec4 v0x564a74f878b0_0;
    %parti/s 4, 0, 2;
    %pad/u 32;
    %cmpi/u 5, 0, 32;
    %flag_inv 5; GE is !LT
    %jmp/0xz  T_0.2, 5;
    %load/vec4 v0x564a74f878b0_0;
    %addi 3, 0, 32;
    %store/vec4 v0x564a74f878b0_0, 0, 32;
T_0.2 ;
    %load/vec4 v0x564a74f878b0_0;
    %parti/s 4, 4, 4;
    %pad/u 32;
    %cmpi/u 5, 0, 32;
    %flag_inv 5; GE is !LT
    %jmp/0xz  T_0.4, 5;
    %load/vec4 v0x564a74f878b0_0;
    %addi 48, 0, 32;
    %store/vec4 v0x564a74f878b0_0, 0, 32;
T_0.4 ;
    %load/vec4 v0x564a74f878b0_0;
    %ix/load 4, 1, 0;
    %flag_set/imm 4, 0;
    %shiftl 4;
    %store/vec4 v0x564a74f878b0_0, 0, 32;
    %load/vec4 v0x564a74f87a50_0;
    %parti/s 1, 4, 4;
    %ix/load 4, 0, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0x564a74f878b0_0, 4, 1;
    %load/vec4 v0x564a74f87a50_0;
    %ix/load 4, 1, 0;
    %flag_set/imm 4, 0;
    %shiftl 4;
    %store/vec4 v0x564a74f87a50_0, 0, 32;
    %load/vec4 v0x564a74f87b80_0;
    %addi 1, 0, 32;
    %store/vec4 v0x564a74f87b80_0, 0, 32;
    %jmp T_0.0;
T_0.1 ;
    %load/vec4 v0x564a74f878b0_0;
    %parti/s 4, 4, 4;
    %store/vec4 v0x564a74f5efc0_0, 0, 4;
    %load/vec4 v0x564a74f878b0_0;
    %parti/s 4, 0, 2;
    %store/vec4 v0x564a74f877d0_0, 0, 4;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x564a74f6f780;
T_1 ;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x564a74f88050_0, 0, 32;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x564a74f88220_0, 0, 32;
T_1.0 ;
    %load/vec4 v0x564a74f88220_0;
    %cmpi/s 31, 0, 32;
    %flag_or 5, 4;
    %jmp/0xz T_1.1, 5;
    %load/vec4 v0x564a74f88220_0;
    %pad/s 5;
    %store/vec4 v0x564a74f87f80_0, 0, 5;
    %load/vec4 v0x564a74f88220_0;
    %pushi/vec4 10, 0, 32;
    %mod/s;
    %pad/s 4;
    %ix/load 4, 0, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0x564a74f88140_0, 4, 4;
    %load/vec4 v0x564a74f88220_0;
    %pushi/vec4 10, 0, 32;
    %div/s;
    %pad/s 4;
    %ix/load 4, 4, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0x564a74f88140_0, 4, 4;
    %delay 1000, 0;
    %load/vec4 v0x564a74f87dc0_0;
    %load/vec4 v0x564a74f87e80_0;
    %concat/vec4; draw_concat_vec4
    %store/vec4 v0x564a74f87ce0_0, 0, 8;
    %load/vec4 v0x564a74f87ce0_0;
    %load/vec4 v0x564a74f88140_0;
    %cmp/ne;
    %jmp/0xz  T_1.2, 6;
    %vpi_call 2 36 "$display", "Error: Input = %0d, Expected BCD = %0h, Actual BCD = %0h", v0x564a74f88220_0, v0x564a74f88140_0, v0x564a74f87ce0_0 {0 0 0};
    %load/vec4 v0x564a74f88050_0;
    %addi 1, 0, 32;
    %store/vec4 v0x564a74f88050_0, 0, 32;
T_1.2 ;
    %load/vec4 v0x564a74f88220_0;
    %addi 1, 0, 32;
    %store/vec4 v0x564a74f88220_0, 0, 32;
    %jmp T_1.0;
T_1.1 ;
    %load/vec4 v0x564a74f88050_0;
    %cmpi/e 0, 0, 32;
    %jmp/0xz  T_1.4, 4;
    %vpi_call 2 44 "$display", "All test cases passed!" {0 0 0};
    %jmp T_1.5;
T_1.4 ;
    %vpi_call 2 46 "$display", "Total errors: %0d", v0x564a74f88050_0 {0 0 0};
T_1.5 ;
    %vpi_call 2 50 "$finish" {0 0 0};
    %end;
    .thread T_1;
# The file index is used to find the file name in the following table.
