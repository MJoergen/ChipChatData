E_0x556744853bd0/0 .event negedge, v0x55674486ffa0_0;
E_0x556744853bd0/1 .event posedge, v0x556744846e20_0;
E_0x556744853bd0 .event/or E_0x556744853bd0/0, E_0x556744853bd0/1;
S_0x5567448700e0 .scope function.vec4.s8, "reference_lfsr_output" "reference_lfsr_output" 2 25, 2 25 0, S_0x556744846c90;
 .timescale -9 -12;
v0x5567448702c0_0 .var "current_state", 7 0;
; Variable reference_lfsr_output is vec4 return value of scope S_0x5567448700e0
TD_tb_lfsr.reference_lfsr_output ;
    %load/vec4 v0x5567448702c0_0;
    %parti/s 7, 0, 2;
    %load/vec4 v0x5567448702c0_0;
    %parti/s 1, 0, 2;
    %load/vec4 v0x5567448702c0_0;
    %parti/s 1, 3, 3;
    %xor;
    %load/vec4 v0x5567448702c0_0;
    %parti/s 1, 5, 4;
    %xor;
    %load/vec4 v0x5567448702c0_0;
    %parti/s 1, 6, 4;
    %xor;
    %concat/vec4; draw_concat_vec4
    %ret/vec4 0, 0, 8;  Assign to reference_lfsr_output (store_vec4_to_lval)
    %end;
    .scope S_0x5567448564c0;
T_1 ;
    %wait E_0x556744853bd0;
    %load/vec4 v0x55674486ffa0_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.0, 8;
    %pushi/vec4 138, 0, 8;
    %assign/vec4 v0x55674486fdd0_0, 0;
    %jmp T_1.1;
T_1.0 ;
    %load/vec4 v0x55674486fdd0_0;
    %parti/s 1, 0, 2;
    %load/vec4 v0x55674486fdd0_0;
    %parti/s 1, 3, 3;
    %xor;
    %load/vec4 v0x55674486fdd0_0;
    %parti/s 1, 5, 4;
    %xor;
    %load/vec4 v0x55674486fdd0_0;
    %parti/s 1, 6, 4;
    %xor;
    %ix/load 4, 0, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0x55674486feb0_0, 4, 1;
    %load/vec4 v0x55674486fdd0_0;
    %parti/s 7, 0, 2;
    %ix/load 4, 1, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0x55674486feb0_0, 4, 7;
    %load/vec4 v0x55674486feb0_0;
    %assign/vec4 v0x55674486fdd0_0, 0;
T_1.1 ;
    %jmp T_1;
    .thread T_1;
    .scope S_0x556744846c90;
T_2 ;
    %delay 5000, 0;
    %load/vec4 v0x556744870480_0;
    %inv;
    %store/vec4 v0x556744870480_0, 0, 1;
    %jmp T_2;
    .thread T_2;
    .scope S_0x556744846c90;
T_3 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x556744870480_0, 0, 1;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5567448707f0_0, 0, 1;
    %delay 5000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5567448707f0_0, 0, 1;
    %load/vec4 v0x556744870570_0;
    %cmpi/ne 138, 0, 8;
    %jmp/0xz  T_3.0, 6;
    %vpi_call 2 43 "$display", "Error: At time %t, data = %b, expected = 10001010", $time, v0x556744870570_0 {0 0 0};
    %vpi_call 2 44 "$finish" {0 0 0};
T_3.0 ;
    %pushi/vec4 138, 0, 8;
    %store/vec4 v0x556744870640_0, 0, 8;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x556744870710_0, 0, 32;
T_3.2 ;
    %load/vec4 v0x556744870710_0;
    %cmpi/s 256, 0, 32;
    %jmp/0xz T_3.3, 5;
    %load/vec4 v0x556744870640_0;
    %store/vec4 v0x5567448702c0_0, 0, 8;
    %callf/vec4 TD_tb_lfsr.reference_lfsr_output, S_0x5567448700e0;
    %store/vec4 v0x556744870640_0, 0, 8;
    %delay 10000, 0;
    %load/vec4 v0x556744870570_0;
    %load/vec4 v0x556744870640_0;
    %cmp/ne;
    %jmp/0xz  T_3.4, 6;
    %vpi_call 2 60 "$display", "Error: At time %t, data = %b, expected = %b", $time, v0x556744870570_0, v0x556744870640_0 {0 0 0};
    %vpi_call 2 61 "$finish" {0 0 0};
T_3.4 ;
    %load/vec4 v0x556744870710_0;
    %addi 1, 0, 32;
    %store/vec4 v0x556744870710_0, 0, 32;
    %jmp T_3.2;
T_3.3 ;
    %vpi_call 2 66 "$display", "Simulation successful. All test cases passed." {0 0 0};
    %vpi_call 2 67 "$finish" {0 0 0};
    %end;
    .thread T_3;
