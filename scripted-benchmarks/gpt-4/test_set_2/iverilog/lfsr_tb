S_0x556744846c90 .scope module, "tb_lfsr" "tb_lfsr" 2 3;
 .timescale -9 -12;
v0x556744870480_0 .var "clk", 0 0;
v0x556744870570_0 .net "data", 7 0, v0x55674486fdd0_0;  1 drivers
v0x556744870640_0 .var "data_expected", 7 0;
v0x556744870710_0 .var/i "i", 31 0;
v0x5567448707f0_0 .var "reset_n", 0 0;
S_0x5567448564c0 .scope module, "lfsr_inst" "lfsr" 2 9, 3 1 0, S_0x556744846c90;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "reset_n";
    .port_info 2 /OUTPUT 8 "data";
P_0x556744843e20 .param/l "TAP1" 1 3 8, +C4<00000000000000000000000000000001>;
P_0x556744843e60 .param/l "TAP2" 1 3 9, +C4<00000000000000000000000000000100>;
P_0x556744843ea0 .param/l "TAP3" 1 3 10, +C4<00000000000000000000000000000110>;
P_0x556744843ee0 .param/l "TAP4" 1 3 11, +C4<00000000000000000000000000000111>;
v0x556744846e20_0 .net "clk", 0 0, v0x556744870480_0;  1 drivers
v0x55674486fdd0_0 .var "data", 7 0;
