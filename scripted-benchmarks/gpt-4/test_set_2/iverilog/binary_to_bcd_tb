#! /usr/bin/vvp
:ivl_version "12.0 (stable)" "(v12_0-dirty)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/usr/lib/ivl/system.vpi";
:vpi_module "/usr/lib/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/ivl/va_math.vpi";
S_0x55b0d9c93020 .scope module, "tb_binary_to_bcd_converter" "tb_binary_to_bcd_converter" 2 3;
 .timescale -9 -12;
v0x55b0d9caab60_0 .net "bcd_output", 7 0, L_0x55b0d9c7f810;  1 drivers
v0x55b0d9caac20_0 .var "binary_input", 4 0;
v0x55b0d9caacc0_0 .var "expected_bcd", 7 0;
v0x55b0d9caad90_0 .var/i "i", 31 0;
v0x55b0d9caae70_0 .var "test_binary", 4 0;
v0x55b0d9caafa0_0 .var "vcd_clk", 0 0;
S_0x55b0d9c931b0 .scope module, "uut" "binary_to_bcd_converter" 2 8, 3 1 0, S_0x55b0d9c93020;
 .timescale 0 0;
    .port_info 0 /INPUT 5 "binary_input";
    .port_info 1 /OUTPUT 8 "bcd_output";
L_0x55b0d9c7f810 .functor BUFZ 8, v0x55b0d9c82bf0_0, C4<00000000>, C4<00000000>, C4<00000000>;
v0x55b0d9c82bf0_0 .var "bcd", 7 0;
v0x55b0d9caa750_0 .net "bcd_output", 7 0, L_0x55b0d9c7f810;  alias, 1 drivers
v0x55b0d9caa830_0 .net "binary_input", 4 0, v0x55b0d9caac20_0;  1 drivers
v0x55b0d9caa8f0_0 .var "binary_temp", 4 0;
v0x55b0d9caa9d0_0 .var/i "i", 31 0;
E_0x55b0d9c90300 .event anyedge, v0x55b0d9caa830_0;
    .scope S_0x55b0d9c931b0;
T_0 ;
    %wait E_0x55b0d9c90300;
    %pushi/vec4 0, 0, 8;
    %store/vec4 v0x55b0d9c82bf0_0, 0, 8;
    %load/vec4 v0x55b0d9caa830_0;
    %store/vec4 v0x55b0d9caa8f0_0, 0, 5;
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x55b0d9caa9d0_0, 0, 32;
T_0.0 ;
    %load/vec4 v0x55b0d9caa9d0_0;
    %cmpi/s 5, 0, 32;
    %jmp/0xz T_0.1, 5;
    %load/vec4 v0x55b0d9c82bf0_0;
    %parti/s 4, 0, 2;
    %pad/u 32;
    %cmpi/u 5, 0, 32;
    %flag_inv 5; GE is !LT
    %jmp/0xz  T_0.2, 5;
    %load/vec4 v0x55b0d9c82bf0_0;
    %parti/s 4, 0, 2;
    %addi 3, 0, 4;
    %ix/load 4, 0, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0x55b0d9c82bf0_0, 4, 4;
T_0.2 ;
    %load/vec4 v0x55b0d9c82bf0_0;
    %parti/s 4, 4, 4;
    %pad/u 32;
    %cmpi/u 5, 0, 32;
    %flag_inv 5; GE is !LT
    %jmp/0xz  T_0.4, 5;
    %load/vec4 v0x55b0d9c82bf0_0;
    %parti/s 4, 4, 4;
    %addi 3, 0, 4;
    %ix/load 4, 4, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0x55b0d9c82bf0_0, 4, 4;
T_0.4 ;
    %load/vec4 v0x55b0d9c82bf0_0;
    %ix/load 4, 1, 0;
    %flag_set/imm 4, 0;
    %shiftl 4;
    %store/vec4 v0x55b0d9c82bf0_0, 0, 8;
    %load/vec4 v0x55b0d9caa8f0_0;
    %parti/s 1, 4, 4;
    %ix/load 4, 0, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0x55b0d9c82bf0_0, 4, 1;
    %load/vec4 v0x55b0d9caa8f0_0;
    %ix/load 4, 1, 0;
    %flag_set/imm 4, 0;
    %shiftl 4;
    %store/vec4 v0x55b0d9caa8f0_0, 0, 5;
    ; show_stmt_assign_vector: Get l-value for compressed += operand
    %load/vec4 v0x55b0d9caa9d0_0;
    %pushi/vec4 1, 0, 32;
    %add;
    %store/vec4 v0x55b0d9caa9d0_0, 0, 32;
    %jmp T_0.0;
T_0.1 ;
    %jmp T_0;
    .thread T_0, $push;
    .scope S_0x55b0d9c93020;
T_1 ;
    %vpi_call 2 18 "$display", "Testing Binary-to-BCD Converter..." {0 0 0};
    %pushi/vec4 0, 0, 32;
    %store/vec4 v0x55b0d9caad90_0, 0, 32;
T_1.0 ;
    %load/vec4 v0x55b0d9caad90_0;
    %cmpi/s 32, 0, 32;
    %jmp/0xz T_1.1, 5;
    %load/vec4 v0x55b0d9caad90_0;
    %pad/s 5;
    %store/vec4 v0x55b0d9caae70_0, 0, 5;
    %load/vec4 v0x55b0d9caae70_0;
    %store/vec4 v0x55b0d9caac20_0, 0, 5;
    %load/vec4 v0x55b0d9caae70_0;
    %pad/u 32;
    %pushi/vec4 10, 0, 32;
    %mod;
    %pad/u 4;
    %ix/load 4, 0, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0x55b0d9caacc0_0, 4, 4;
    %load/vec4 v0x55b0d9caae70_0;
    %pad/u 32;
    %pushi/vec4 10, 0, 32;
    %div;
    %pad/u 4;
    %ix/load 4, 4, 0;
    %flag_set/imm 4, 0;
    %store/vec4 v0x55b0d9caacc0_0, 4, 4;
    %delay 10000, 0;
    %load/vec4 v0x55b0d9caab60_0;
    %load/vec4 v0x55b0d9caacc0_0;
    %cmp/ne;
    %jmp/0xz  T_1.2, 6;
    %vpi_call 2 31 "$display", "Error: Test case %0d failed. Expected BCD: 8'b%0b, Got: 8'b%0b", v0x55b0d9caae70_0, v0x55b0d9caacc0_0, v0x55b0d9caab60_0 {0 0 0};
    %vpi_call 2 33 "$finish" {0 0 0};
T_1.2 ;
    ; show_stmt_assign_vector: Get l-value for compressed += operand
    %load/vec4 v0x55b0d9caad90_0;
    %pushi/vec4 1, 0, 32;
    %add;
    %store/vec4 v0x55b0d9caad90_0, 0, 32;
    %jmp T_1.0;
T_1.1 ;
    %vpi_call 2 37 "$display", "All test cases passed!" {0 0 0};
    %vpi_call 2 38 "$finish" {0 0 0};
    %end;
    .thread T_1;
    .scope S_0x55b0d9c93020;
T_2 ;
    %vpi_call 2 43 "$dumpfile", "my_design.vcd" {0 0 0};
    %vpi_call 2 44 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x55b0d9c93020 {0 0 0};
    %end;
    .thread T_2;
    .scope S_0x55b0d9c93020;
T_3 ;
    %delay 5000, 0;
    %load/vec4 v0x55b0d9caafa0_0;
    %inv;
    %store/vec4 v0x55b0d9caafa0_0, 0, 1;
    %jmp T_3;
    .thread T_3;
# The file index is used to find the file name in the following table.
